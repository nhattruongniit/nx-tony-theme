{
  "root": "libs/themes/minimal",
  "sourceRoot": "libs/themes/minimal/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/themes/minimal",
        "tsConfig": "libs/themes/minimal/tsconfig.lib.json",
        "project": "libs/themes/minimal/package.json",
        "entryFile": "libs/themes/minimal/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/themes/minimal/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/themes/minimal/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/themes/minimal"],
      "options": {
        "jestConfig": "libs/themes/minimal/jest.config.js",
        "passWithNoTests": true
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm publish",
        "cwd": "dist/libs/themes/minimal"
      }
    }
  }
}
